    "recharts": "^2.13.3",
    "toaster-ts": "^0.3.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.3.3",
    "prisma": "^5.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
